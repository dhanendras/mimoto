{
    "credential_issuer": "https://idt.one1sewa.com",
    "credential_endpoint": "https://idt.one1sewa.com/v1/esignet/vci/credential",
    "credentials_supported": [
        {
            "format": "ldp_vc",
            "id": "EducationCredential",
            "scope": "sunbird_rc_education_vc_ldp",
            "cryptographic_binding_methods_supported": [
                "did:jwk"
            ],
            "cryptographic_suites_supported": [
                "Ed25519Signature2020"
            ],
            "proof_types_supported": [
                "jwt"
            ],
            "credential_definition": {
                "type": [
                    "VerifiableCredential"
                ],
                "credentialSubject": {
                    "enrollmentNumber": {
                      "display": [
                        {
                          "name": "enrollment Number",
                          "locale": "en"
                        }
                      ]
                    },
                    "studentName": {
                      "display": [
                        {
                          "name": "student Name",
                          "locale": "en"
                        }
                      ]
                    },
                    "instituteName": {
                      "display": [
                        {
                          "name": "institute Name",
                          "locale": "en"
                        }
                      ]
                    },
                    "gender": {
                      "display": [
                        {
                          "name": "Gender",
                          "locale": "en"
                        }
                      ]
                    },
                    "expiresOn": {
                      "display": [
                        {
                          "name": "Expiry Date",
                          "locale": "en"
                        }
                      ]
                    },
                    "dob": {
                      "display": [
                        {
                          "name": "Date Of Birth",
                          "locale": "en"
                        }
                      ]
                    }
                  }
            },
            "order": [
                "enrollmentNumber",
                "studentName",
                "expiresOn",
                "instituteName",                
                "dob",
                "gender"
            ],
            "display": [
                {
                    "name": "Sunbird RC Education Verifiable Credential",
                    "locale": "en",
                    "logo": {
                        "url": "https://assets.kpmg.com/is/image/kpmg/kpmg-logo-1",
                        "alt_text": "a square logo of a Sunbird"
                    },
                    "background_color": "#FDFAF9",
                    "text_color": "#7C4616"
                }
            ]
        }
    ],
    "display": [
        {
            "name": "Education",
            "locale": "en"
        }
    ]
}